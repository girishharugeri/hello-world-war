pipeline {
  agent none
  stages {
    stage ( 'my build' ) {
      agent { label 'slave_devops' }
      steps {
          sh 'pwd'
          sh 'whoami'
          sh "echo ${BUILD_NUMBER}"
          sh 'mvn deploy'
       }
     }  
          stage ( 'my deploy' ) {
            agent { label 'slave_git' }
              steps {
                sh 'whoami'
                sh 'curl -u harugeri.girish@gmail.com:Raibag@74 -O https://jfrogiri.jfrog.io/artifactory/libs-release-local/com/efsavage/hello-world-war/${BUILD_NUMBER}/hello-world-war-${BUILD_NUMBER}.war'
                sh 'sudo cp -R hello-world-war-${BUILD_NUMBER}.war /opt/apache-tomcat-10.0.27/webapps/'
                sh 'sudo tomcatdown'
                sh 'sleep 3'
                sh 'sudo tomcatup'
      }   
    }
  }
}
